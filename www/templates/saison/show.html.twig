{% extends 'base.html.twig' %}

{% block title %}Saison{% endblock %}

{% block body %}
    <h1>Saison</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ saison.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ saison.label }}</td>
            </tr>
            <tr>
                <th>DateStart</th>
                <td>{{ saison.dateStart ? saison.dateStart|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateEnd</th>
                <td>{{ saison.dateEnd ? saison.dateEnd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_saison_index') }}">back to list</a>

    <a href="{{ path('app_saison_edit', {'id': saison.id}) }}">edit</a>

    {{ include('saison/_delete_form.html.twig') }}
{% endblock %}
